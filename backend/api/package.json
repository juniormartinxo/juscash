{
  "name": "dje-management-api",
  "version": "1.0.0",
  "description": "API para gerenciamento de publicações do DJE de São Paulo",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "NODE_OPTIONS='--require tsconfig-paths/register' nodemon --exec tsx src/app.ts",
    "dev:debug": "nodemon --exec tsx --inspect=0.0.0.0:9229 src/app.ts",
    "lint": "npx @biomejs/biome check --write",
    "lint:fix": "npx @biomejs/biome check --write --unsafe",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:migrate:reset": "npx prisma migrate reset",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "tsx scripts/seed.ts",
    "db:backup": "./scripts/backup.sh",
    "db:restore": "./scripts/restore.sh",
    "docker:build": "docker build -t dje-api .",
    "docker:run": "docker run -p 3001:3001 --env-file .env dje-api",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f api",
    "docker:clean": "docker system prune -f",
    "setup": "chmod +x scripts/*.sh && ./scripts/dev-setup.sh",
    "monitor": "./scripts/monitor.sh",
    "load-test": "k6 run scripts/load-test.js",
    "security-audit": "pnpm audit --audit-level moderate",
    "precommit": "pnpm lint && pnpm type-check && pnpm test:unit",
    "prepare": "husky install || true",
    "health": "curl -f http://localhost:3001/health || exit 1",
    "clean": "rm -rf dist coverage node_modules/.cache"
  },
  "keywords": [
    "dje",
    "justice",
    "scraping",
    "api",
    "nodejs",
    "express",
    "prisma",
    "postgresql",
    "typescript",
    "clean-architecture"
  ],
  "author": "Junior Martins <amjr.box@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@prisma/client": "6.9.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.9.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "overrides": {
      "semver": "^7.5.4"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@prisma/client",
      "@prisma/engines",
      "@scarf/scarf",
      "bcrypt",
      "esbuild",
      "prisma"
    ]
  },
  "resolutions": {
    "express": "^5.1.0",
    "express-async-errors": "^3.1.1"
  }
}